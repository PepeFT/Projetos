Algoritmo "TESTE_CONDICIONAL"

Var

     hora : inteiro



Inicio
      escreva ("Digite um horario do dia: ")
      leia(hora)
      
      se hora < 12 entao
         escreval ("Bom dia!")
      senao
           se(hora <18)entao
              escreval ("Boa Tarde!")
           senao
                escreval ("Boa Noite")
           Fimse
      Fimse


Fimalgoritmo